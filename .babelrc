{
  "presets": [
    ["es2015",
    {
      "modules": false
    }], "stage-0", "react"
  ],
  "plugins": [
    "transform-runtime",
    "react-hot-loader/babel",
    "transform-decorators-legacy", ["import",
    {
      "libraryName": "antd",
      "style": "true"
    }],
  ],
  "env":
  {
    "production":
    {
      "presets": ["react-optimize"]
    }
  }
}